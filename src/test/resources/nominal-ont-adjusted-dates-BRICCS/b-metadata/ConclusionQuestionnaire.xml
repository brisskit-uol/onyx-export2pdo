<?xml version="1.0" encoding="UTF-8"?>
<om:source xmlns:om="http://brisskit.org/xml/onyxmetadata/v1.0/om">
   <om:name>onyx</om:name>
   <om:stage>
      <om:name>ConclusionQuestionnaire</om:name>
      <om:variable>
         <om:name>QuestionnaireRun</om:name>
         <om:variable>
            <om:name>version</om:name>
            <om:type>text</om:type>
         </om:variable>
         <om:variable>
            <om:name>locale</om:name>
            <om:type>locale</om:type>
         </om:variable>
         <om:variable>
            <om:name>timeStart</om:name>
            <om:type>datetime</om:type>
         </om:variable>
         <om:variable>
            <om:name>timeEnd</om:name>
            <om:type>datetime</om:type>
         </om:variable>
         <om:variable>
            <om:name>user</om:name>
            <om:type>text</om:type>
         </om:variable>
      </om:variable>
      <om:variable>
         <om:name>QuestionnaireMetric</om:name>
         <om:variable>
            <om:name>section</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>page</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>duration</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>questionCount</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>missingCount</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
      </om:variable>
      <om:section>
         <om:name>CHECKLIST</om:name>
         <om:question>
            <om:name>CONCLUSION</om:name>
            <om:label>This concludes the data collection process for this patient. Please ensure all notes are returned to the Medical Records Department.</om:label>
            <om:variable>
               <om:name>CONCLUSION</om:name>
               <om:type>text</om:type>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>epi_dischmeth</om:name>
            <om:label>Discharge Method</om:label>
            <om:variable>
               <om:name>epi_dischmeth</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Home</om:name>
                  <om:label>Home</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Other_hospital</om:name>
                  <om:label>Other hospital</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Convalescence</om:name>
                  <om:label>Convalescence</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Death</om:name>
                  <om:label>Death</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Other_specialty</om:name>
                  <om:label>Other specialty in same hospital</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Unknown</om:name>
                  <om:label>Unknown</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>drugs_discharge</om:name>
            <om:label>Drugs on discharge or departure from clinic</om:label>
            <om:variable>
               <om:name>drugs_discharge</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>NONE</om:name>
                  <om:label>None</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Aspirin</om:name>
                  <om:label>Aspirin</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>ACEi_ARB</om:name>
                  <om:label>ACEi / ARB</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>B_blocker</om:name>
                  <om:label>B-blocker</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Statin</om:name>
                  <om:label>Statin</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Thienopyridine</om:name>
                  <om:label>Thienopyridine inhibitor</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Nitrate</om:name>
                  <om:label>Nitrate</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Aldosterone</om:name>
                  <om:label>Aldosterone antagonist</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Warfarin</om:name>
                  <om:label>Warfarin</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Calcium_blocker</om:name>
                  <om:label>Calcium channel blocker</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Insulin</om:name>
                  <om:label>Insulin</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Oral_hypoglycaemic</om:name>
                  <om:label>Oral hypoglycaemic agent</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Others</om:name>
                  <om:label>Others</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Unknown</om:name>
                  <om:label>Unknown</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
      </om:section>
   </om:stage>
</om:source>