<?xml version="1.0" encoding="UTF-8"?>
<om:source xmlns:om="http://brisskit.org/xml/onyxmetadata/v1.0/om">
   <om:name>onyx</om:name>
   <om:stage>
      <om:name>BloodSamplesCollection</om:name>
      <om:variable>
         <om:name>ParticipantTubeRegistration</om:name>
         <om:variable>
            <om:name>otherContraindication</om:name>
            <om:type>text</om:type>
         </om:variable>
         <om:variable>
            <om:name>endTime</om:name>
            <om:type>datetime</om:type>
         </om:variable>
         <om:variable>
            <om:name>Contraindication</om:name>
            <om:variable>
               <om:name>type</om:name>
               <om:type>text</om:type>
               <om:restriction>
                  <om:enum>ASKED</om:enum>
                  <om:enum>OBSERVED</om:enum>
               </om:restriction>
            </om:variable>
         </om:variable>
         <om:variable>
            <om:name>startTime</om:name>
            <om:type>datetime</om:type>
         </om:variable>
      </om:variable>
      <om:variable>
         <om:name>RegisteredParticipantTube</om:name>
         <om:variable>
            <om:name>comment</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>remark</om:name>
            <om:variable>
               <om:name>Samples_wrong_order</om:name>
               <om:type>boolean</om:type>
               <om:repeatable>true</om:repeatable>
            </om:variable>
            <om:variable>
               <om:name>Sufficient_sample</om:name>
               <om:type>boolean</om:type>
               <om:repeatable>true</om:repeatable>
            </om:variable>
            <om:variable>
               <om:name>Difficult_venepuncture</om:name>
               <om:type>boolean</om:type>
               <om:repeatable>true</om:repeatable>
            </om:variable>
            <om:variable>
               <om:name>Haemolysed_sample</om:name>
               <om:type>boolean</om:type>
               <om:repeatable>true</om:repeatable>
            </om:variable>
            <om:variable>
               <om:name>Insufficient_sample</om:name>
               <om:type>boolean</om:type>
               <om:repeatable>true</om:repeatable>
            </om:variable>
            <om:variable>
               <om:name>Blood_in_urine</om:name>
               <om:type>boolean</om:type>
               <om:repeatable>true</om:repeatable>
            </om:variable>
         </om:variable>
         <om:variable>
            <om:name>registrationTime</om:name>
            <om:type>datetime</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>tubeCode</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>barcode</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>prefixCode</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
      </om:variable>
   </om:stage>
</om:source>