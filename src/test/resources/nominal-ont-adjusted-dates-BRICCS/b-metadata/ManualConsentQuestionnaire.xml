<?xml version="1.0" encoding="UTF-8"?>
<om:source xmlns:om="http://brisskit.org/xml/onyxmetadata/v1.0/om">
   <om:name>onyx</om:name>
   <om:stage>
      <om:name>ManualConsentQuestionnaire</om:name>
      <om:variable>
         <om:name>QuestionnaireRun</om:name>
         <om:variable>
            <om:name>version</om:name>
            <om:type>text</om:type>
         </om:variable>
         <om:variable>
            <om:name>locale</om:name>
            <om:type>locale</om:type>
         </om:variable>
         <om:variable>
            <om:name>timeStart</om:name>
            <om:type>datetime</om:type>
         </om:variable>
         <om:variable>
            <om:name>timeEnd</om:name>
            <om:type>datetime</om:type>
         </om:variable>
         <om:variable>
            <om:name>user</om:name>
            <om:type>text</om:type>
         </om:variable>
      </om:variable>
      <om:variable>
         <om:name>QuestionnaireMetric</om:name>
         <om:variable>
            <om:name>section</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>page</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>duration</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>questionCount</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>missingCount</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
      </om:variable>
      <om:section>
         <om:name>MAIN</om:name>
         <om:question>
            <om:name>consent_q1</om:name>
            <om:label>Does the participant understand the request for consent?</om:label>
            <om:variable>
               <om:name>consent_q1</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>consent_q2</om:name>
            <om:label>Does the participant consent to donate blood and urine?</om:label>
            <om:variable>
               <om:name>consent_q2</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>consent_q3</om:name>
            <om:label>Does the participant consent to entry in the BRICCS database?</om:label>
            <om:variable>
               <om:name>consent_q3</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>consent_q4</om:name>
            <om:label>Does the participant consent to possible further contact from BRU?</om:label>
            <om:variable>
               <om:name>consent_q4</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>consent_q5</om:name>
            <om:label>Does the participant understand the rules for withdrawal?</om:label>
            <om:variable>
               <om:name>consent_q5</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
      </om:section>
   </om:stage>
</om:source>